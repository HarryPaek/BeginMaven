<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%5p [%t] - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <logger name="spms.dao" level="TRACE" additivity="false">
            <appender-ref ref="console" />
        </logger>
        <Root level="ERROR">
            <AppenderRef ref="console" />
        </Root>
    </Loggers>
</Configuration>
